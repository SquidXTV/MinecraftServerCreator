<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.squidxtv.msc.controller.MenuController">
    <AnchorPane VBox.vgrow="ALWAYS">
        <Label alignment="CENTER" layoutX="237.0" layoutY="78.0" prefHeight="50.0" prefWidth="427.0" style="&#10;"
               text="Minecraft Server Creator">
            <font>
                <Font size="28.0"/>
            </font>
        </Label>
        <Label layoutX="-1.0" layoutY="474.0" prefHeight="29.0" prefWidth="143.0"
               text=" VERSION: prototype!">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <Label layoutX="-1.0" layoutY="453.0" prefHeight="21.0" prefWidth="162.0" text=" Connor Schweighoefer">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <Button layoutX="351.0" layoutY="260.0" mnemonicParsing="false" prefHeight="50.0"
                prefWidth="198.0" text="CREATE SERVER" textAlignment="CENTER" textOverrun="CLIP" onAction="#createServerButtonAction">
            <font>
                <Font size="22.0"/>
            </font>
        </Button>
        <Button layoutX="351.0" layoutY="330.0" mnemonicParsing="false" prefHeight="50.0"
                prefWidth="198.0" text="SERVER LIST" textAlignment="CENTER" textOverrun="CLIP" onAction="#serverListButtonAction">
            <font>
                <Font size="22.0"/>
            </font>
        </Button>
        <Button fx:id="exitButton" layoutX="351.0" layoutY="400.0" mnemonicParsing="false" prefHeight="50.0"
                prefWidth="198.0" text="EXIT" textAlignment="CENTER" textOverrun="CLIP" onAction="#exitButtonAction">
            <font>
                <Font size="22.0"/>
            </font>
        </Button>
    </AnchorPane>
</VBox>