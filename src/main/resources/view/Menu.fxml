<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.squidxtv.msc.controller.MenuController">
    <AnchorPane VBox.vgrow="ALWAYS">
        <!--Title-->
        <Label alignment="CENTER" layoutX="238.0" layoutY="89.0" prefHeight="50.0" prefWidth="427.0" style="&#10;" text="Minecraft Server Creator">
            <font>
                <Font size="28.0" />
            </font>
        </Label>
        <!--Version Label-->
        <Label layoutX="-1.0" layoutY="474.0" prefHeight="29.0" prefWidth="143.0" text=" VERSION: prototype!">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <!--Author Label-->
        <Label layoutX="-1.0" layoutY="453.0" prefHeight="21.0" prefWidth="250.0" text=" Connor Schweighoefer/SquidXTV">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <!--Create Server Button-->
        <Button onAction="#createServerAction" layoutX="352.0" layoutY="205.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="198.0" text="CREATE SERVER" textAlignment="CENTER" textOverrun="CLIP">
            <font>
                <Font size="22.0" />
            </font>
        </Button>
        <!--Server List Button-->
        <Button onAction="#serverListAction" layoutX="352.0" layoutY="275.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="198.0" text="SERVER LIST" textAlignment="CENTER" textOverrun="CLIP">
            <font>
                <Font size="22.0" />
            </font>
        </Button>
        <!--Exit Button-->
        <Button fx:id="exit" onAction="#exitAction" layoutX="352.0" layoutY="345.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="198.0" text="EXIT" textAlignment="CENTER" textOverrun="CLIP">
            <font>
                <Font size="22.0" />
            </font>
        </Button>
        <!--Settings Button-->
        <Button onAction="#settingsAction" fx:id="settings" layoutX="814.0" layoutY="421.0" mnemonicParsing="false" prefHeight="48.0" prefWidth="48.0" />
    </AnchorPane>
</VBox>
